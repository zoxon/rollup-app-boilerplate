{
  "name": "rollup-app-boilerplate",
  "description": "Minimal boilerplate for starting app with rollup.js",
  "version": "1.0.0",
  "main": "rollup.config.js",
  "author": {
    "name" : "Velichko Konstantin",
    "url" : "https://github.com/zoxon"
  },
  "keywords": [
    "rollup", 
    "javascript", 
    "app", 
    "boilerplate", 
    "postcss", 
    "browsercync"
  ],
  "license": "MIT",
  "scripts": {
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist",
    "build": "rollup -c",
    "build:watch": "npm run build -- -w",
    "dev": "npm run build:watch",
    "start": "npm run dev"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "normalize.css": "^8.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-1": "^6.24.1",
    "npm-run-all": "^4.1.2",
    "perfectionist": "^2.4.0",
    "postcss-advanced-variables": "^2.3.2",
    "postcss-calc": "^6.0.1",
    "postcss-color-hex-alpha": "^3.0.0",
    "postcss-color-hsl": "^2.0.0",
    "postcss-color-hwb": "^3.0.0",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-import": "^11.1.0",
    "postcss-nested": "^3.0.0",
    "postcss-pseudoelements": "^5.0.0",
    "postcss-reporter": "^5.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.56.2",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-browsersync": "^0.2.6",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-fill-html": "^1.1.0",
    "rollup-plugin-image": "^1.0.2",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-postcss": "^1.2.8",
    "rollup-plugin-uglify": "^3.0.0"
  }
}
